{
  "private": true,
  "scripts": {
    "with-env": "dotenv -e .env --",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ensure:docker": "sh scripts/ensure-docker.sh",
    "ensure:nabostylisten-db": "sh scripts/ensure-nabostylisten-db.sh",
    "seed:sync": "bunx @snaplet/seed sync",
    "seed": "bunx tsx seed/seed.ts > supabase/seed.sql",
    "seed:stripe": "NODE_ENV=development bun run with-env bun scripts/seed-stripe.ts",
    "supabase:start": "bun ensure:docker && supabase start",
    "supabase:db:diff": "bun ensure:docker && supabase db diff -f",
    "supabase:migrate:up": "bun ensure:docker && supabase migration up",
    "supabase:db:push": "supabase db push",
    "supabase:db:reset": "supabase db reset && bun run scripts/supabase/buckets.ts create && bun run with-env bun scripts/supabase/stripe-sync-engine.ts && bun run seed:stripe",
    "supabase:db:reset:no-seed": "supabase db reset --no-seed && bun run scripts/supabase/buckets.ts create",
    "supabase:db:types": "bun ensure:docker && bun ensure:nabostylisten-db && supabase gen types typescript --local --schema public,stripe,gis > types/database.types.ts",
    "supabase:db:types:zod": "supazod -i types/database.types.ts -o schemas/database.schema.ts -s public,stripe,gis",
    "remove:public:prefix": "tsx scripts/remove-public-prefix.ts",
    "supabase:stripe:generate-sync-migrations": "bun run scripts/supabase/stripe-sync-engine.ts",
    "supabase:emails:export": "sh scripts/export-email-templates.sh",
    "supabase:functions:deploy": "supabase functions deploy",
    "supabase:load:secrets": "sh supabase secrets set --env-file ./supabase/.env",
    "gen:types": "bun run supabase:db:types && bun run supabase:db:types:zod && bun run remove:public:prefix",
    "buckets:create": "bun run scripts/supabase/buckets.ts create",
    "buckets:list": "bun run scripts/supabase/buckets.ts list",
    "buckets:delete": "bun run scripts/supabase/buckets.ts delete",
    "assets:upload": "bun run scripts/supabase/upload-assets.ts upload",
    "assets:list": "bun run scripts/supabase/upload-assets.ts list",
    "assets:delete": "bun run scripts/supabase/upload-assets.ts delete",
    "email:dev": "cd transactional && bun run dev",
    "email:build": "cd transactional && bun run build",
    "email:export": "cd transactional && bun run export",
    "stripe:functions:serve": "supabase functions serve --env-file ./supabase/functions/.env",
    "stripe:listen": "stripe listen --forward-to localhost:54321/functions/v1/stripe-sync",
    "stripe:webhook:dev": "./scripts/stripe-webhook-dev.sh",
    "stripe:webhook:identity": "stripe listen --forward-to localhost:3000/api/webhooks/stripe/identity",
    "stripe:trigger": "stripe trigger",
    "stripe:test:connect": "bun run with-env bun scripts/test-stripe-connect.ts",
    "test:dev:cron": "sh scripts/test-dev-cron.sh",
    "migration:full": "sh scripts/run-full-migration.sh",
    "migration:phase": "bun run with-env bun scripts/migration/run-phase-with-dump.ts"
  },
